{
    "gitbook": "3.1.1",
    "title": "Structuring Angular Apps with Angular Material Components",
    "plugins": [
      "include-codeblock",
      "advanced-emoji",
      "prism",
      "highlight",
      "copy-code-button",
      "edit-link",
      "github",
      "github-buttons"
    ],
    "pluginsConfig": {
      "edit-link": {
        "base": "https://github.com/btroncone/egghead-material2/tree/master",
        "label": "Edit This Page"
      },
      "github": {
        "url": "https://github.com/btroncone/egghead-material2/"
      },
      "github-buttons": {
        "repo": "btroncone/egghead-material2",
        "types": [
          "star",
          "watch"
        ],
        "size": "large"
      }
    }
  }